int arr[n];
cout<<"enter number in array : ";
for(int i=0 ; i<n ; i++){
    cin>>arr[i];
}

int max=INT_MIN;

for(int i=0 ; i<n ; i++){
    if(arr[i]>max){
        max=arr[i];
    }

    else if(max >arr[i]){
       
    }


}

cout<<max;

    return 0;
}
