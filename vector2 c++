#include<iostream>
#include<vector>
using namespace std;
int main(){
    vector<int> v={1,2,3,4,5,6,7};

   //adding Elements using push_back in vector  
   v.push_back(8);
   v.push_back(9);
   v.push_back(10);

// access elements
for(int val : v){
    cout<<val<<" ";
}

cout<<endl;

//access elements
for(int i=0 ; i<v.size() ; i++){
    cout<<v[i]<<" ";

}

// access first elements
cout<<endl<<"FIRST ELEMENT : "<<v[0]<<endl;

//access second element
cout<<"Second Element: "<<v[1]<<endl;


cout<<endl;

//Removing the last elements 
 v.pop_back();

 cout<<"After Removing Last Element: ";
 for(int i=0 ;i<v.size(); i++){
    cout<<v[i]<<" ";
 }

 //again remove last element
 v.pop_back();

 cout<<endl<<"again After Removing last Element: ";
 for(int itr : v){
    cout<<itr <<" ";
 }

    return 0;
}
