#include<iostream>
#include<vector>
using namespace std;
int main(){
    vector<int> v={1,2,3,4,5,6,7};

   //adding Elements using push_back in vector  
   v.push_back(8);
   v.push_back(9);
   v.push_back(10);

for(int val : v){
    cout<<val<<" ";
}

cout<<endl;

for(int i=0 ; i<v.size() ; i++){
    cout<<v[i]<<" ";

}

    return 0;
}
